{
  "name": "audiom-embedder",
  "version": "1.0.0",
  "description": "A strongly-typed TypeScript client library for embedding Audiom inclusive mapping solutions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build",
    "clean": "rmdir /s /q dist 2>nul || exit 0"
  },
  "keywords": [
    "audiom",
    "maps",
    "accessibility",
    "embed",
    "typescript"
  ],
  "author": "XR Navigation Inc.",
  "license": "UNLICENSED",
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
    "typescript": "^5.3.0",
    "vitest": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16"
  },
  "repository": {
    "type": "git",
    "url": ""
  }
}
